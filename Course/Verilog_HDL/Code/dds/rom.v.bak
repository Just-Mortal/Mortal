module rom (
    input wire clk, // 时钟信号
    input wire [13:0] addr, // 12位地址输入
    output reg [11:0] q // 12位数据输出
);


reg [13:0] rom[12287:0];

initial begin
    // 读取MIF文件并初始化ROM数组
    $readmemh("/home/wink/nju_digital_circuits/DDS/wave.dat", rom);
end

always @(posedge clk) begin
    // 根据地址异步读取数据
    q <= rom[addr];
end

endmodule